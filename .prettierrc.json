{
  "semi": true,
  "trailingComma": "all",
  "singleQuote": true,
  "tabWidth": 2,
  "printWidth": 120,
  "plugins": ["prettier-plugin-tailwindcss", "@ianvs/prettier-plugin-sort-imports"],
  "importOrder": [
    "^(react/(.*)$)|^(react$)",
    "^(next/(.*)$)|^(next$)",
    "<THIRD_PARTY_MODULES>",
    "",
    "^constants/(.*)$",
    "^types/(.*)$",
    "",
    "^libs/(.*)$",
    "^utils/(.*)$",
    "",
    "^hooks/(.*)$",
    "^components/(.*)$",
    "",
    "^styles/(.*)$",
    "",
    "^stores/(.*)$",
    "^providers/(.*)$",
    "",
    "^features/(.*)$",
    "^page-components/(.*)$",
    "^pages/(.*)$",
    "",
    "^[./]"
  ]
}
